<%- include('../partials/header') %>

<h2>Recipe List</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        <% recipes.forEach(function(r) { %>
            <tr>
            <td><%= r.name %></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>
